# task 4

import itertools
for p in itertools.permutations("012345", 3):
    print(('').join(str(x) for x in p))
print()
for p in itertools.combinations('012345', 3):
    print(('').join(p))
print()
for p in itertools.product('012345', repeat=3):
    print(('').join(p))
print()
