# task 2
1.

import numpy as np
m = 0
n = 0
for i in range(0,1000):
    x = np.random.uniform(0,10)
    if x < 5 :
        m += 1
    else :
        n += 1
print (m/1000 + n/1)

2.

import matplotlib.pyplot as plt
import numpy as np
y = 0
for i in range(10):
    x = np.random.rand(1000)
    y += x
print(y)
plt.hist(y)
plt.xlabel('y')
plt.ylabel('Probability')
plt.show()

